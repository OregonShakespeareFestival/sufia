
<h2 class="non lower">Descriptions</h2>
<table class="table table-striped"><!-- class="verticalheadings"> -->
  <tbody>
  <% present_terms(@presenter) do |r, term| %>
      <tr>
        <th><%= r.label(term) %></th>
        <td><%= r.value(term) %></td>
      </tr>
  <% end %>
  </tbody>
</table> <!-- /verticalheadings -->
